{
  "name": "base-multiple",
  "version": "1.0.0",
  "description": "build a workspace",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:log": "gulp sprite && webpack --config webpack.config.prod.js --profile --json > stats.json",
    "build:test": "gulp sprite && cross-env NODE_ENV=test webpack --config webpack.config.prod.js",
    "build:prod": "gulp sprite && cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "serve": "webpack-dev-server --config webpack.config.dev.js",
    "start": "webpack-dev-server --open --config webpack.config.dev.js",
    "sprite": "gulp sprite",
    "minImages": "gulp minImages"
  },
  "keywords": [
    "workspace"
  ],
  "author": "liangjun",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-import": "^1.13.0",
    "cache-loader": "^2.0.1",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^6.0.3",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.0.2",
    "gulp": "^4.0.0",
    "gulp-tinypng-nokey": "^1.1.0",
    "gulp.spritesmith": "^6.9.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "thread-loader": "^2.1.2",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.3",
    "vconsole": "^3.3.0",
    "vinyl-buffer": "^1.0.1",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "view-design": "^4.2.0",
    "vue": "^2.6.11"
  }
}
